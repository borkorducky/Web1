        }

        function retakeTest() {
            document.getElementById('resultContainer').classList.add('hidden');
            initializeTestDisplay();
        }

        function viewHistory() {
            if (!currentUser) {
                Swal.fire({
                    icon: 'warning',
                    title: '‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏Ç‡πâ‡∏≤‡∏™‡∏π‡πà‡∏£‡∏∞‡∏ö‡∏ö',
                    text: '‡∏Ñ‡∏∏‡∏ì‡∏ï‡πâ‡∏≠‡∏á‡πÄ‡∏Ç‡πâ‡∏≤‡∏™‡∏π‡πà‡∏£‡∏∞‡∏ö‡∏ö‡∏Å‡πà‡∏≠‡∏ô‡∏î‡∏π‡∏õ‡∏£‡∏∞‡∏ß‡∏±‡∏ï‡∏¥‡∏Å‡∏≤‡∏£‡∏ó‡∏î‡∏™‡∏≠‡∏ö',
                    confirmButtonText: '‡πÄ‡∏Ç‡πâ‡∏≤‡∏™‡∏π‡πà‡∏£‡∏∞‡∏ö‡∏ö',
                    confirmButtonColor: '#3b82f6'
                }).then((result) => {
                    if (result.isConfirmed) {
                        showLogin();
                    }
                });
                return;
            }

            // Filter test results for current user
            const userTests = allTestResults.filter(test => 
                test.userId === currentUser.id || test.userEmail === currentUser.email
            );
            
            if (userTests.length > 0) {
                displayHistoryResults(userTests, '‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏Å‡∏≤‡∏£‡∏ó‡∏î‡∏™‡∏≠‡∏ö');
            } else {
                Swal.fire({
                    icon: 'info',
                    title: '‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏õ‡∏£‡∏∞‡∏ß‡∏±‡∏ï‡∏¥‡∏Å‡∏≤‡∏£‡∏ó‡∏î‡∏™‡∏≠‡∏ö',
                    text: '‡∏Ñ‡∏∏‡∏ì‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÄ‡∏Ñ‡∏¢‡∏ó‡∏≥‡πÅ‡∏ö‡∏ö‡∏ó‡∏î‡∏™‡∏≠‡∏ö ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡∏ó‡∏≥‡πÅ‡∏ö‡∏ö‡∏ó‡∏î‡∏™‡∏≠‡∏ö‡∏Å‡πà‡∏≠‡∏ô',
                    confirmButtonText: '‡∏ó‡∏≥‡πÅ‡∏ö‡∏ö‡∏ó‡∏î‡∏™‡∏≠‡∏ö',
                    confirmButtonColor: '#3b82f6'
                }).then((result) => {
                    if (result.isConfirmed) {
                        document.getElementById('test').scrollIntoView({behavior: 'smooth'});
                    }
                });
            }
        }



        function displayHistoryResults(userTests, dataSource) {
            // Sort by date (newest first)
            userTests.sort((a, b) => new Date(b.testDate) - new Date(a.testDate));
            
            let historyHtml = `
                <div class="text-center mb-4">
                    <span class="inline-block px-3 py-1 bg-blue-100 text-blue-800 rounded-full text-sm">
                        üìä ‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏à‡∏≤‡∏Å: ${dataSource}
                    </span>
                </div>
                <div class="max-h-96 overflow-y-auto">
            `;
            
            userTests.forEach((test, index) => {
                const date = new Date(test.testDate).toLocaleDateString('th-TH', {
                    year: 'numeric',
                    month: 'long',
                    day: 'numeric'
                });
                const time = new Date(test.testDate).toLocaleTimeString('th-TH', {
                    hour: '2-digit',
                    minute: '2-digit'
                });
                
                // Calculate top skills
                const topSkills = Object.entries(test.skillScores || {})
                    .sort(([,a], [,b]) => b - a)
                    .slice(0, 3)
                    .map(([skill, score]) => `${skillCategories[skill] || skill}: ${score}%`)
                    .join(', ');
                
                const totalMinutes = Math.floor((test.totalTime || 0) / 60);
                const totalSeconds = (test.totalTime || 0) % 60;
                
                historyHtml += `
                    <div class="border border-gray-200 rounded-lg p-4 mb-3 hover:bg-gray-50 transition-colors">
                        <div class="flex justify-between items-start mb-2">
                            <h4 class="font-bold text-gray-800">‡∏Å‡∏≤‡∏£‡∏ó‡∏î‡∏™‡∏≠‡∏ö‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà ${index + 1}</h4>
                            <span class="text-xs bg-green-100 text-green-800 px-2 py-1 rounded">
                                ‚úÖ ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à
                            </span>
                        </div>
                        <div class="grid grid-cols-2 gap-2 text-sm">
                            <div>
                                <span class="text-gray-500">üìÖ ‡∏ß‡∏±‡∏ô‡∏ó‡∏µ‡πà:</span>
                                <span class="font-medium">${date}</span>
                            </div>
                            <div>
                                <span class="text-gray-500">üïê ‡πÄ‡∏ß‡∏•‡∏≤:</span>
                                <span class="font-medium">${time}</span>
                            </div>
                        </div>
                        <div class="mt-2">
                            <span class="text-gray-500 text-sm">üèÜ ‡∏ó‡∏±‡∏Å‡∏©‡∏∞‡πÄ‡∏î‡πà‡∏ô:</span>
                            <p class="text-sm font-medium text-blue-600">${topSkills || '‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•'}</p>
                        </div>
                        <div class="mt-2 flex justify-between items-center">
                            <span class="text-xs text-gray-500">
                                ‚è±Ô∏è ‡πÄ‡∏ß‡∏•‡∏≤‡∏ó‡∏µ‡πà‡πÉ‡∏ä‡πâ: ${totalMinutes}:${totalSeconds.toString().padStart(2, '0')}
                            </span>
                            <button onclick="viewDetailedResult('${test.testId || index}')" 
                                    class="text-xs bg-blue-500 text-white px-2 py-1 rounded hover:bg-blue-600">
                                ‡∏î‡∏π‡∏£‡∏≤‡∏¢‡∏•‡∏∞‡πÄ‡∏≠‡∏µ‡∏¢‡∏î
                            </button>
                        </div>
                    </div>
                `;
            });
            
            historyHtml += '</div>';
            
            Swal.fire({
                title: `üìã ‡∏õ‡∏£‡∏∞‡∏ß‡∏±‡∏ï‡∏¥‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡πÅ‡∏ö‡∏ö‡∏ó‡∏î‡∏™‡∏≠‡∏ö (${userTests.length} ‡∏Ñ‡∏£‡∏±‡πâ‡∏á)`,
                html: historyHtml,
                width: 800,
                confirmButtonText: '‡∏õ‡∏¥‡∏î',
                confirmButtonColor: '#3b82f6',
                showCancelButton: true,
                cancelButtonText: '‡∏ó‡∏≥‡πÅ‡∏ö‡∏ö‡∏ó‡∏î‡∏™‡∏≠‡∏ö‡πÉ‡∏´‡∏°‡πà',
                cancelButtonColor: '#10b981'
            }).then((result) => {
                if (result.dismiss === Swal.DismissReason.cancel) {
                    document.getElementById('test').scrollIntoView({behavior: 'smooth'});
                    if (currentUser) {
                        startTest();
                    }
                }
            });
        }

        function viewDetailedResult(testId) {
            Swal.fire({
                icon: 'info',
                title: '‡∏£‡∏≤‡∏¢‡∏•‡∏∞‡πÄ‡∏≠‡∏µ‡∏¢‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏î‡∏™‡∏≠‡∏ö',
                text: '‡∏ü‡∏µ‡πÄ‡∏à‡∏≠‡∏£‡πå‡∏ô‡∏µ‡πâ‡∏à‡∏∞‡∏û‡∏±‡∏í‡∏ô‡∏≤‡πÉ‡∏ô‡πÄ‡∏ß‡∏≠‡∏£‡πå‡∏ä‡∏±‡∏ô‡∏ñ‡∏±‡∏î‡πÑ‡∏õ',
                confirmButtonText: '‡∏ï‡∏Å‡∏•‡∏á'
            });
        }

        // Initialize test display when user is logged in
        function initializeTestDisplay() {
            if (currentUser) {
                document.getElementById('loginPrompt').classList.add('hidden');
                document.getElementById('testContainer').classList.remove('hidden');
                
                // Show start button instead of auto-starting
                document.getElementById('questionContainer').innerHTML = `
                    <div class="text-center py-12">
                        <div class="text-6xl mb-6">üìù</div>
                        <h3 class="text-2xl font-bold text-gray-800 mb-4">‡∏û‡∏£‡πâ‡∏≠‡∏°‡∏ó‡∏≥‡πÅ‡∏ö‡∏ö‡∏ó‡∏î‡∏™‡∏≠‡∏ö‡πÅ‡∏•‡πâ‡∏ß‡∏´‡∏£‡∏∑‡∏≠‡∏¢‡∏±‡∏á?</h3>
                        <p class="text-gray-600 mb-8 max-w-2xl mx-auto">
                            ‡∏°‡∏µ‡∏Ñ‡∏≥‡∏ñ‡∏≤‡∏° 24 ‡∏Ç‡πâ‡∏≠ ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏õ‡∏£‡∏∞‡πÄ‡∏°‡∏¥‡∏ô‡∏ó‡∏±‡∏Å‡∏©‡∏∞‡πÉ‡∏ô‡∏î‡πâ‡∏≤‡∏ô‡∏ï‡πà‡∏≤‡∏á‡πÜ ‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì
                        </p>
                        <div class="flex flex-col sm:flex-row gap-4 justify-center items-center">
                            <button onclick="startActualTest()" class="bg-blue-600 text-white px-8 py-4 rounded-lg text-lg font-semibold hover:bg-blue-700 transition-colors">
                                üöÄ ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ó‡∏≥‡πÅ‡∏ö‡∏ö‡∏ó‡∏î‡∏™‡∏≠‡∏ö
                            </button>
                            <button onclick="viewHistory()" class="bg-green-600 text-white px-6 py-4 rounded-lg text-lg font-semibold hover:bg-green-700 transition-colors">
                                üìã ‡∏î‡∏π‡∏õ‡∏£‡∏∞‡∏ß‡∏±‡∏ï‡∏¥‡∏Å‡∏≤‡∏£‡∏ó‡∏î‡∏™‡∏≠‡∏ö
                            </button>
                        </div>
                    </div>
                `;
            }
        }

        // Function to actually start the test
        function startActualTest() {
            currentQuestionIndex = 0;
            answers = [];
            startTime = new Date();
            
            // Reset the question container to show the actual test interface
            document.getElementById('questionContainer').innerHTML = `
                <div class="mb-6">
                    <div class="flex justify-between items-center mb-4">
                        <span class="text-sm text-gray-500">‡∏Ñ‡∏≥‡∏ñ‡∏≤‡∏°‡∏ó‡∏µ‡πà <span id="currentQuestion">1</span> ‡∏à‡∏≤‡∏Å 24</span>
                    </div>
                    <div class="w-full bg-gray-200 rounded-full h-2">
                        <div id="progressBar" class="bg-blue-600 h-2 rounded-full progress-bar" style="width: 4%"></div>
                    </div>
                </div>
                
                <div id="questionContent">
                    <h3 id="questionText" class="text-xl font-bold text-gray-800 mb-6"></h3>
                    <div id="answerOptions" class="space-y-4"></div>
                </div>
                
                <div class="flex justify-between mt-8">
                    <button id="prevBtn" onclick="previousQuestion()" class="bg-gray-300 text-gray-700 px-6 py-3 rounded-lg hover:bg-gray-400 transition-colors" disabled>‡∏Å‡πà‡∏≠‡∏ô‡∏´‡∏ô‡πâ‡∏≤</button>
                    <button id="nextBtn" onclick="nextQuestion()" class="bg-blue-600 text-white px-6 py-3 rounded-lg hover:bg-blue-700 transition-colors">‡∏ñ‡∏±‡∏î‡πÑ‡∏õ</button>
                </div>
            `;
            
            startTimer();
            showQuestion();
        }
