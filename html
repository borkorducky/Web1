    <script>
        // Global variables
        let currentUser = null;
        let currentQuestionIndex = 0;
        let answers = [];
        let startTime = null;
        let timerInterval = null;
        let allUsers = [];
        let allTestResults = [];
        
        const GOOGLE_SCRIPT_URL = 'https://script.google.com/macros/s/AKfycbyA2iveKXodkY2O86bP5_ukFa6oC_7VcFGeytrl3ON1URtuTbv0HFXkeeZk2zMClscgLA/exec';
        const GOOGLE_SHEET_ID = '1n-ZXCBrtfihD73oNimNGZLVM9dUd2aeCZFKnhgfesjQ';
        
        // JSONP request function for Google Apps Script
        function makeJSONPRequest(url, params, callback) {
            const callbackName = 'jsonp_callback_' + Math.round(100000 * Math.random());
            
            // Create callback function
            window[callbackName] = function(data) {
                delete window[callbackName];
                if (document.body.contains(script)) {
                    document.body.removeChild(script);
                }
                callback(data);
            };
            
            // Build URL with parameters
            const queryString = Object.keys(params)
                .map(key => encodeURIComponent(key) + '=' + encodeURIComponent(params[key]))
                .join('&');
            
            const finalUrl = url + '?' + queryString + '&callback=' + callbackName;
            
            // Create script tag
            const script = document.createElement('script');
            script.src = finalUrl;
            script.onerror = function() {
                delete window[callbackName];
                if (document.body.contains(script)) {
                    document.body.removeChild(script);
                }
                callback({ success: false, error: 'Network connection failed' });
            };
            
            // Set timeout for request
            setTimeout(() => {
                if (window[callbackName]) {
                    delete window[callbackName];
                    if (document.body.contains(script)) {
                        document.body.removeChild(script);
                    }
                    callback({ success: false, error: 'Request timeout' });
                }
            }, 10000); // 10 second timeout
            
            document.body.appendChild(script);
        }
        
