        // Load all data from Google Sheet
        function loadDataFromGoogleSheet() {
            console.log('üîÑ Loading data from Google Sheet...');
            
            makeJSONPRequest(GOOGLE_SCRIPT_URL, { action: 'loadAllData' }, function(response) {
                
                if (response && response.success) {
                    console.log('‚úÖ Data loaded successfully from Google Sheet');
                    
                    // Store data globally
                    allUsers = response.users || [];
                    allTestResults = response.testResults || [];
                    
                    // Store in localStorage as backup
                    localStorage.setItem('allUsers', JSON.stringify(allUsers));
                    localStorage.setItem('allTestResults', JSON.stringify(allTestResults));
                    
                    // Check if user was previously logged in
                    checkLoginStatus();
                } else {
                    console.log('‚ùå Failed to load from Google Sheet:', response);
                    loadDataFromLocalStorage();
                }
            });
        }
        
        // Load data from localStorage as fallback
        function loadDataFromLocalStorage() {
            allUsers = JSON.parse(localStorage.getItem('allUsers') || '[]');
            allTestResults = JSON.parse(localStorage.getItem('allTestResults') || '[]');
            
            // Add demo users if no users exist
            if (allUsers.length === 0) {
                allUsers = [
                    {
                        id: 'demo1',
                        name: '‡∏ú‡∏π‡πâ‡πÉ‡∏ä‡πâ‡∏ó‡∏î‡∏™‡∏≠‡∏ö',
                        email: 'demo@test.com',
                        password: '123456',
                        registeredAt: new Date().toISOString()
                    },
                    {
                        id: 'admin1',
                        name: '‡∏ú‡∏π‡πâ‡∏î‡∏π‡πÅ‡∏•‡∏£‡∏∞‡∏ö‡∏ö',
                        email: 'admin@skillup.com',
                        password: 'admin123',
                        registeredAt: new Date().toISOString()
                    }
                ];
                localStorage.setItem('allUsers', JSON.stringify(allUsers));
            }
            
            checkLoginStatus();
        }
        
        // Show connection status (disabled)
        function showConnectionStatus(message, type) {
            // Status display disabled
            return;
        }

        // Initialize the application
        document.addEventListener('DOMContentLoaded', function() {
            createDemandChart();
            loadDataFromGoogleSheet();
        });

        // Authentication functions
        function showLogin() {
            document.getElementById('loginModal').classList.remove('hidden');
        }

        function showRegister() {
            document.getElementById('registerModal').classList.remove('hidden');
        }

        function closeModal(modalId) {
            document.getElementById(modalId).classList.add('hidden');
        }

        function checkLoginStatus() {
            const savedUser = localStorage.getItem('currentUser');
            if (savedUser) {
                currentUser = JSON.parse(savedUser);
                updateUIForLoggedInUser();
            }
        }

        function updateUIForLoggedInUser() {
            if (currentUser) {
                // Update navigation
                const navButtons = document.querySelector('nav .flex.space-x-1.md\\:space-x-2');
                if (navButtons) {
                    navButtons.innerHTML = `
                        <span class="text-white text-sm md:text-base">‡∏™‡∏ß‡∏±‡∏™‡∏î‡∏µ, ${currentUser.name}</span>
                        <button onclick="logout()" class="bg-red-600 text-white px-2 md:px-4 py-2 rounded-lg hover:bg-red-700 transition-colors text-sm md:text-base">‡∏≠‡∏≠‡∏Å‡∏à‡∏≤‡∏Å‡∏£‡∏∞‡∏ö‡∏ö</button>
                    `;
                }
                
                // Initialize test display
                initializeTestDisplay();
            }
        }

        function logout() {
            currentUser = null;
            localStorage.removeItem('currentUser');
            location.reload();
        }



        // Handle login form
        document.getElementById('loginForm').addEventListener('submit', function(e) {
            e.preventDefault();
            
            const email = document.getElementById('loginEmail').value.trim();
            const password = document.getElementById('loginPassword').value;
            
            console.log('Login attempt:', email);
            console.log('Available users:', allUsers.length);
            
            // Validation
            if (!email || !password) {
                Swal.fire({
                    icon: 'warning',
                    title: '‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡∏Å‡∏£‡∏≠‡∏Å‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡πÉ‡∏´‡πâ‡∏Ñ‡∏£‡∏ö‡∏ñ‡πâ‡∏ß‡∏ô',
                    text: '‡πÇ‡∏õ‡∏£‡∏î‡∏Å‡∏£‡∏≠‡∏Å‡∏≠‡∏µ‡πÄ‡∏°‡∏•‡πÅ‡∏•‡∏∞‡∏£‡∏´‡∏±‡∏™‡∏ú‡πà‡∏≤‡∏ô'
                });
                return;
            }
            
            // Check user credentials
            const user = allUsers.find(u => u.email === email && u.password === password);
            console.log('Found user:', user);
            
            if (user) {
                currentUser = {
                    id: user.id,
                    name: user.name,
                    email: user.email
                };
                localStorage.setItem('currentUser', JSON.stringify(currentUser));
                console.log('User logged in:', currentUser);
                
                // Clear form
                document.getElementById('loginForm').reset();
                
                Swal.fire({
                    icon: 'success',
                    title: '‡πÄ‡∏Ç‡πâ‡∏≤‡∏™‡∏π‡πà‡∏£‡∏∞‡∏ö‡∏ö‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!',
                    text: `‡∏¢‡∏¥‡∏ô‡∏î‡∏µ‡∏ï‡πâ‡∏≠‡∏ô‡∏£‡∏±‡∏ö ${currentUser.name}`,
                    timer: 2000,
                    showConfirmButton: false
                });
                
                closeModal('loginModal');
                updateUIForLoggedInUser();
            } else {
                console.log('Login failed - user not found');
                Swal.fire({
                    icon: 'error',
                    title: '‡πÄ‡∏Ç‡πâ‡∏≤‡∏™‡∏π‡πà‡∏£‡∏∞‡∏ö‡∏ö‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à',
                    text: '‡∏≠‡∏µ‡πÄ‡∏°‡∏•‡∏´‡∏£‡∏∑‡∏≠‡∏£‡∏´‡∏±‡∏™‡∏ú‡πà‡∏≤‡∏ô‡πÑ‡∏°‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡∏•‡∏≠‡∏á‡πÉ‡∏´‡∏°‡πà‡∏≠‡∏µ‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á'
                });
            }
        });

        // Handle register form
        document.getElementById('registerForm').addEventListener('submit', function(e) {
            e.preventDefault();
            
            const name = document.getElementById('registerName').value.trim();
            const email = document.getElementById('registerEmail').value.trim();
            const password = document.getElementById('registerPassword').value;
            
            // Validation
            if (!name || !email || !password) {
                Swal.fire({
                    icon: 'warning',
                    title: '‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡∏Å‡∏£‡∏≠‡∏Å‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡πÉ‡∏´‡πâ‡∏Ñ‡∏£‡∏ö‡∏ñ‡πâ‡∏ß‡∏ô',
                    text: '‡πÇ‡∏õ‡∏£‡∏î‡∏Å‡∏£‡∏≠‡∏Å‡∏ä‡∏∑‡πà‡∏≠ ‡∏≠‡∏µ‡πÄ‡∏°‡∏• ‡πÅ‡∏•‡∏∞‡∏£‡∏´‡∏±‡∏™‡∏ú‡πà‡∏≤‡∏ô'
                });
                return;
            }
            
            if (password.length < 6) {
                Swal.fire({
                    icon: 'warning',
                    title: '‡∏£‡∏´‡∏±‡∏™‡∏ú‡πà‡∏≤‡∏ô‡∏™‡∏±‡πâ‡∏ô‡πÄ‡∏Å‡∏¥‡∏ô‡πÑ‡∏õ',
                    text: '‡∏£‡∏´‡∏±‡∏™‡∏ú‡πà‡∏≤‡∏ô‡∏ï‡πâ‡∏≠‡∏á‡∏°‡∏µ‡∏≠‡∏¢‡πà‡∏≤‡∏á‡∏ô‡πâ‡∏≠‡∏¢ 6 ‡∏ï‡∏±‡∏ß‡∏≠‡∏±‡∏Å‡∏©‡∏£'
                });
                return;
            }
            
            const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
            if (!emailRegex.test(email)) {
                Swal.fire({
                    icon: 'warning',
                    title: '‡∏£‡∏π‡∏õ‡πÅ‡∏ö‡∏ö‡∏≠‡∏µ‡πÄ‡∏°‡∏•‡πÑ‡∏°‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á',
                    text: '‡πÇ‡∏õ‡∏£‡∏î‡∏Å‡∏£‡∏≠‡∏Å‡∏≠‡∏µ‡πÄ‡∏°‡∏•‡πÉ‡∏ô‡∏£‡∏π‡∏õ‡πÅ‡∏ö‡∏ö‡∏ó‡∏µ‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á'
                });
                return;
            }
            
            // Check if email already exists
            if (allUsers.find(user => user.email === email)) {
                Swal.fire({
                    icon: 'error',
                    title: '‡∏≠‡∏µ‡πÄ‡∏°‡∏•‡∏ô‡∏µ‡πâ‡∏ñ‡∏π‡∏Å‡πÉ‡∏ä‡πâ‡πÅ‡∏•‡πâ‡∏ß',
                    text: '‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÉ‡∏ä‡πâ‡∏≠‡∏µ‡πÄ‡∏°‡∏•‡∏≠‡∏∑‡πà‡∏ô ‡∏´‡∏£‡∏∑‡∏≠‡πÄ‡∏Ç‡πâ‡∏≤‡∏™‡∏π‡πà‡∏£‡∏∞‡∏ö‡∏ö‡∏´‡∏≤‡∏Å‡∏Ñ‡∏∏‡∏ì‡∏°‡∏µ‡∏ö‡∏±‡∏ç‡∏ä‡∏µ‡πÅ‡∏•‡πâ‡∏ß'
                });
                return;
            }
            
            // Create new user object
            const newUser = {
                id: 'USER_' + Date.now(),
                name: name,
                email: email,
                password: password,
                registeredAt: new Date().toISOString()
            };
            
            // Save to Google Sheet
            makeJSONPRequest(GOOGLE_SCRIPT_URL, {
                action: 'registerUser',
                userData: JSON.stringify(newUser)
            }, function(result) {
                // Clear form
                document.getElementById('registerForm').reset();
                
                // Add to local array
                allUsers.push(newUser);
                localStorage.setItem('allUsers', JSON.stringify(allUsers));
                
                Swal.fire({
                    icon: 'success',
                    title: '‡∏™‡∏°‡∏±‡∏Ñ‡∏£‡∏™‡∏°‡∏≤‡∏ä‡∏¥‡∏Å‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!',
                    html: `
                        <p class="mb-4">‡∏ö‡∏±‡∏ç‡∏ä‡∏µ‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì‡∏ñ‡∏π‡∏Å‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢‡πÅ‡∏•‡πâ‡∏ß</p>
                        <div class="bg-blue-50 p-4 rounded-lg text-left">
                            <p><strong>‡∏ä‡∏∑‡πà‡∏≠:</strong> ${name}</p>
                            <p><strong>‡∏≠‡∏µ‡πÄ‡∏°‡∏•:</strong> ${email}</p>
                        </div>
                    `,
                    confirmButtonText: '‡πÄ‡∏Ç‡πâ‡∏≤‡∏™‡∏π‡πà‡∏£‡∏∞‡∏ö‡∏ö',
                    confirmButtonColor: '#3b82f6'
                }).then(() => {
                    closeModal('registerModal');
                    // Auto-fill login form
                    document.getElementById('loginEmail').value = email;
                    document.getElementById('loginPassword').value = password;
                    showLogin();
                });
            });
        });

        // Skills test functions
        function startTest() {
            if (!currentUser) {
                showLogin();
                return;
            }
            
            // Initialize test display instead of starting immediately
            initializeTestDisplay();
        }

        function startTimer() {
            timerInterval = setInterval(() => {
                const now = new Date();
                const elapsed = Math.floor((now - startTime) / 1000);
                const minutes = Math.floor(elapsed / 60);
                const seconds = elapsed % 60;
                document.getElementById('timer').textContent = 
                    `${minutes.toString().padStart(2, '0')}:${seconds.toString().padStart(2, '0')}`;
            }, 1000);
        }

        function showQuestion() {
            const question = questions[currentQuestionIndex];
            document.getElementById('questionText').textContent = question.text;
            document.getElementById('currentQuestion').textContent = currentQuestionIndex + 1;
            
            const progress = ((currentQuestionIndex + 1) / questions.length) * 100;
            document.getElementById('progressBar').style.width = `${progress}%`;
            
            const optionsContainer = document.getElementById('answerOptions');
            optionsContainer.innerHTML = '';
            
            question.options.forEach((option, index) => {
                const optionDiv = document.createElement('div');
                optionDiv.className = 'flex items-center p-4 border border-gray-200 rounded-lg hover:bg-blue-50 cursor-pointer transition-colors';
                optionDiv.onclick = () => selectAnswer(index);
                
                optionDiv.innerHTML = `
                    <input type="radio" name="answer" value="${index}" class="mr-3">
                    <label class="cursor-pointer flex-1">${option.text}</label>
                `;
                
                optionsContainer.appendChild(optionDiv);
            });
            
            // Update button states
            document.getElementById('prevBtn').disabled = currentQuestionIndex === 0;
            document.getElementById('nextBtn').textContent = 
                currentQuestionIndex === questions.length - 1 ? '‡πÄ‡∏™‡∏£‡πá‡∏à‡∏™‡∏¥‡πâ‡∏ô' : '‡∏ñ‡∏±‡∏î‡πÑ‡∏õ';
        }

        function selectAnswer(optionIndex) {
            const radios = document.querySelectorAll('input[name="answer"]');
            radios[optionIndex].checked = true;
            
            // Update visual selection
            document.querySelectorAll('#answerOptions > div').forEach((div, index) => {
                if (index === optionIndex) {
                    div.classList.add('bg-blue-50', 'border-blue-300');
                } else {
                    div.classList.remove('bg-blue-50', 'border-blue-300');
                }
            });
        }

        function nextQuestion() {
            const selectedAnswer = document.querySelector('input[name="answer"]:checked');
            if (!selectedAnswer) {
                Swal.fire({
                    icon: 'warning',
                    title: '‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏Ñ‡∏≥‡∏ï‡∏≠‡∏ö',
                    text: '‡πÇ‡∏õ‡∏£‡∏î‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏Ñ‡∏≥‡∏ï‡∏≠‡∏ö‡∏Å‡πà‡∏≠‡∏ô‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏Ñ‡∏≥‡∏ñ‡∏≤‡∏°‡∏ñ‡∏±‡∏î‡πÑ‡∏õ'
                });
                return;
            }
            
            answers[currentQuestionIndex] = parseInt(selectedAnswer.value);
            
            if (currentQuestionIndex === questions.length - 1) {
                finishTest();
            } else {
                currentQuestionIndex++;
                showQuestion();
            }
        }

        function previousQuestion() {
            if (currentQuestionIndex > 0) {
                currentQuestionIndex--;
                showQuestion();
                
                // Restore previous answer if exists
                if (answers[currentQuestionIndex] !== undefined) {
                    const radios = document.querySelectorAll('input[name="answer"]');
                    radios[answers[currentQuestionIndex]].checked = true;
                    selectAnswer(answers[currentQuestionIndex]);
                }
            }
        }

        async function finishTest() {
            clearInterval(timerInterval);
            
            const endTime = new Date();
            const totalTime = Math.floor((endTime - startTime) / 1000);
            
            // Calculate skill scores
            const skillScores = {};
            Object.keys(skillCategories).forEach(skill => {
                skillScores[skill] = 0;
            });
            
            answers.forEach((answerIndex, questionIndex) => {
                const question = questions[questionIndex];
                const selectedOption = question.options[answerIndex];
                selectedOption.skills.forEach(skill => {
                    skillScores[skill] += 1;
                });
            });
            
            // Convert to percentages
            const maxScore = Math.max(...Object.values(skillScores));
            Object.keys(skillScores).forEach(skill => {
                skillScores[skill] = Math.round((skillScores[skill] / maxScore) * 100);
            });
            
            // Save test result
            const testResult = {
                userId: currentUser.id,
                userEmail: currentUser.email,
                userName: currentUser.name,
                testDate: new Date().toISOString(),
                totalTime: totalTime,
                answers: answers,
                skillScores: skillScores
            };
            
            saveTestResult(testResult);
            showResults(skillScores, totalTime);
        }

        function saveTestResult(testResult) {
            // Generate test ID
            testResult.testId = 'TEST_' + Date.now();
            
            // Save to Google Sheet using JSONP
            makeJSONPRequest(GOOGLE_SCRIPT_URL, {
                action: 'saveTestResult',
                testData: JSON.stringify(testResult)
            }, function(result) {
                // Add to local array
                allTestResults.push(testResult);
                localStorage.setItem('allTestResults', JSON.stringify(allTestResults));
                
                Swal.fire({
                    icon: 'success',
                    title: '‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏ú‡∏•‡∏Å‡∏≤‡∏£‡∏ó‡∏î‡∏™‡∏≠‡∏ö‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!',
                    html: `
                        <div class="text-center">
                            <div class="text-6xl mb-4">üéâ</div>
                            <p class="mb-2">‡∏ú‡∏•‡∏Å‡∏≤‡∏£‡∏ó‡∏î‡∏™‡∏≠‡∏ö‡∏ñ‡∏π‡∏Å‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢‡πÅ‡∏•‡πâ‡∏ß</p>
                            <div class="bg-green-50 p-3 rounded-lg mt-4">
                                <p class="text-green-600 text-xs">‡∏£‡∏´‡∏±‡∏™‡∏Å‡∏≤‡∏£‡∏ó‡∏î‡∏™‡∏≠‡∏ö: ${testResult.testId}</p>
                            </div>
                        </div>
                    `,
                    timer: 3000,
                    showConfirmButton: false
                });
            });
        }

        function showResults(skillScores, totalTime) {
            document.getElementById('questionContainer').classList.add('hidden');
            document.getElementById('resultContainer').classList.remove('hidden');
            
            // Create skill chart
            createSkillChart(skillScores);
            
            // Show skill breakdown
            const breakdownContainer = document.getElementById('skillBreakdown');
            breakdownContainer.innerHTML = '';
            
            Object.entries(skillScores).forEach(([skill, score]) => {
                const skillDiv = document.createElement('div');
                skillDiv.className = 'bg-gray-50 p-4 rounded-lg';
                skillDiv.innerHTML = `
                    <h4 class="font-bold text-gray-800 mb-2">${skillCategories[skill]}</h4>
                    <div class="w-full bg-gray-200 rounded-full h-3 mb-2">
                        <div class="bg-blue-600 h-3 rounded-full progress-bar" style="width: ${score}%"></div>
                    </div>
                    <p class="text-sm text-gray-600">${score}% - ${getSkillRecommendation(skill, score)}</p>
                `;
                breakdownContainer.appendChild(skillDiv);
            });
            
            // Show completion time
            const minutes = Math.floor(totalTime / 60);
            const seconds = totalTime % 60;
            const timeDiv = document.createElement('div');
            timeDiv.className = 'col-span-full text-center mt-4 p-4 bg-blue-50 rounded-lg';
            timeDiv.innerHTML = `
                <h4 class="font-bold text-blue-600 mb-2">‡πÄ‡∏ß‡∏•‡∏≤‡∏ó‡∏µ‡πà‡πÉ‡∏ä‡πâ‡πÉ‡∏ô‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡πÅ‡∏ö‡∏ö‡∏ó‡∏î‡∏™‡∏≠‡∏ö</h4>
                <p class="text-2xl font-bold text-blue-800">${minutes}:${seconds.toString().padStart(2, '0')}</p>
            `;
            breakdownContainer.appendChild(timeDiv);

            // Show career recommendations
            const careerRecommendations = getCareerRecommendations(skillScores);
            if (careerRecommendations.length > 0) {
                const careerDiv = document.createElement('div');
                careerDiv.className = 'col-span-full mt-4 p-4 bg-green-50 rounded-lg';
                careerDiv.innerHTML = `
                    <h4 class="font-bold text-green-600 mb-3">üíº ‡∏≠‡∏≤‡∏ä‡∏µ‡∏û‡∏ó‡∏µ‡πà‡πÅ‡∏ô‡∏∞‡∏ô‡∏≥‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Ñ‡∏∏‡∏ì</h4>
                    <div class="grid grid-cols-2 md:grid-cols-3 gap-2">
                        ${careerRecommendations.map(career => 
                            `<div class="bg-white p-2 rounded text-sm text-center border border-green-200">${career}</div>`
                        ).join('')}
                    </div>
                    <p class="text-xs text-green-600 mt-2">*‡∏≠‡∏¥‡∏á‡∏à‡∏≤‡∏Å‡∏ó‡∏±‡∏Å‡∏©‡∏∞‡∏ó‡∏µ‡πà‡πÄ‡∏î‡πà‡∏ô‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì</p>
                `;
                breakdownContainer.appendChild(careerDiv);
            }
        }

        function getSkillRecommendation(skill, score) {
            if (score >= 80) return "‡∏¢‡∏≠‡∏î‡πÄ‡∏¢‡∏µ‡πà‡∏¢‡∏°! ‡∏Ñ‡∏∏‡∏ì‡∏°‡∏µ‡∏ó‡∏±‡∏Å‡∏©‡∏∞‡πÉ‡∏ô‡∏î‡πâ‡∏≤‡∏ô‡∏ô‡∏µ‡πâ‡∏™‡∏π‡∏á‡∏°‡∏≤‡∏Å";
            if (score >= 60) return "‡∏î‡∏µ‡∏°‡∏≤‡∏Å! ‡∏Ñ‡∏ß‡∏£‡∏û‡∏±‡∏í‡∏ô‡∏≤‡∏ï‡πà‡∏≠‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏ä‡∏µ‡πà‡∏¢‡∏ß‡∏ä‡∏≤‡∏ç";
            if (score >= 40) return "‡∏õ‡∏≤‡∏ô‡∏Å‡∏•‡∏≤‡∏á ‡∏Ñ‡∏ß‡∏£‡∏ù‡∏∂‡∏Å‡∏ù‡∏ô‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏ï‡∏¥‡∏°";
            return "‡∏Ñ‡∏ß‡∏£‡πÉ‡∏´‡πâ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏≥‡∏Ñ‡∏±‡∏ç‡πÅ‡∏•‡∏∞‡∏û‡∏±‡∏í‡∏ô‡∏≤‡∏ó‡∏±‡∏Å‡∏©‡∏∞‡∏ô‡∏µ‡πâ";
        }

        function getCareerRecommendations(skillScores) {
            const careers = {
                financial: ["‡∏ô‡∏±‡∏Å‡∏ß‡∏¥‡πÄ‡∏Ñ‡∏£‡∏≤‡∏∞‡∏´‡πå‡∏Å‡∏≤‡∏£‡πÄ‡∏á‡∏¥‡∏ô", "‡∏ó‡∏µ‡πà‡∏õ‡∏£‡∏∂‡∏Å‡∏©‡∏≤‡∏Å‡∏≤‡∏£‡πÄ‡∏á‡∏¥‡∏ô", "‡∏ô‡∏±‡∏Å‡∏ö‡∏±‡∏ç‡∏ä‡∏µ", "‡∏ô‡∏±‡∏Å‡∏•‡∏á‡∏ó‡∏∏‡∏ô"],
                eq: ["‡∏ú‡∏π‡πâ‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏£‡∏±‡∏û‡∏¢‡∏≤‡∏Å‡∏£‡∏ö‡∏∏‡∏Ñ‡∏Ñ‡∏•", "‡∏ô‡∏±‡∏Å‡∏à‡∏¥‡∏ï‡∏ß‡∏¥‡∏ó‡∏¢‡∏≤", "‡∏ú‡∏π‡πâ‡∏ô‡∏≥‡∏ó‡∏µ‡∏°", "‡∏ô‡∏±‡∏Å‡∏Ç‡∏≤‡∏¢"],
                analysis: ["‡∏ô‡∏±‡∏Å‡∏ß‡∏¥‡πÄ‡∏Ñ‡∏£‡∏≤‡∏∞‡∏´‡πå‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•", "‡∏ô‡∏±‡∏Å‡∏ß‡∏¥‡∏à‡∏±‡∏¢", "‡∏ó‡∏µ‡πà‡∏õ‡∏£‡∏∂‡∏Å‡∏©‡∏≤‡∏ò‡∏∏‡∏£‡∏Å‡∏¥‡∏à", "‡∏ô‡∏±‡∏Å‡∏ß‡∏¥‡πÄ‡∏Ñ‡∏£‡∏≤‡∏∞‡∏´‡πå‡∏£‡∏∞‡∏ö‡∏ö"],
                time: ["‡∏ú‡∏π‡πâ‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡πÇ‡∏Ñ‡∏£‡∏á‡∏Å‡∏≤‡∏£", "‡∏ú‡∏π‡πâ‡∏õ‡∏£‡∏∞‡∏™‡∏≤‡∏ô‡∏á‡∏≤‡∏ô", "‡∏ú‡∏π‡πâ‡∏ö‡∏£‡∏¥‡∏´‡∏≤‡∏£", "‡∏ú‡∏π‡πâ‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏™‡∏≥‡∏ô‡∏±‡∏Å‡∏á‡∏≤‡∏ô"],
                communication: ["‡∏ô‡∏±‡∏Å‡∏õ‡∏£‡∏∞‡∏ä‡∏≤‡∏™‡∏±‡∏°‡∏û‡∏±‡∏ô‡∏ò‡πå", "‡∏ô‡∏±‡∏Å‡∏Ç‡πà‡∏≤‡∏ß", "‡∏Ñ‡∏£‡∏π/‡∏≠‡∏≤‡∏à‡∏≤‡∏£‡∏¢‡πå", "‡∏ô‡∏±‡∏Å‡∏Å‡∏≤‡∏£‡∏ï‡∏•‡∏≤‡∏î"],
                coding: ["‡∏ô‡∏±‡∏Å‡∏û‡∏±‡∏í‡∏ô‡∏≤‡∏ã‡∏≠‡∏ü‡∏ï‡πå‡πÅ‡∏ß‡∏£‡πå", "‡∏ú‡∏π‡πâ‡πÄ‡∏ä‡∏µ‡πà‡∏¢‡∏ß‡∏ä‡∏≤‡∏ç‡∏Ñ‡∏ß‡∏≤‡∏°‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢", "‡∏ß‡∏¥‡∏®‡∏ß‡∏Å‡∏£‡∏£‡∏∞‡∏ö‡∏ö", "‡∏ô‡∏±‡∏Å‡∏ß‡∏¥‡πÄ‡∏Ñ‡∏£‡∏≤‡∏∞‡∏´‡πå‡∏£‡∏∞‡∏ö‡∏ö"],
                marketing: ["‡∏ô‡∏±‡∏Å‡∏Å‡∏≤‡∏£‡∏ï‡∏•‡∏≤‡∏î‡∏î‡∏¥‡∏à‡∏¥‡∏ó‡∏±‡∏•", "‡∏ú‡∏π‡πâ‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡πÅ‡∏ö‡∏£‡∏ô‡∏î‡πå", "‡∏ô‡∏±‡∏Å‡πÇ‡∏Ü‡∏©‡∏ì‡∏≤", "‡∏ú‡∏π‡πâ‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏™‡∏∑‡πà‡∏≠‡∏™‡∏±‡∏á‡∏Ñ‡∏°"],
                technical: ["‡∏ä‡πà‡∏≤‡∏á‡πÄ‡∏ó‡∏Ñ‡∏ô‡∏¥‡∏Ñ", "‡∏ß‡∏¥‡∏®‡∏ß‡∏Å‡∏£", "‡∏ä‡πà‡∏≤‡∏á‡∏ã‡πà‡∏≠‡∏°‡∏ö‡∏≥‡∏£‡∏∏‡∏á", "‡∏ú‡∏π‡πâ‡πÄ‡∏ä‡∏µ‡πà‡∏¢‡∏ß‡∏ä‡∏≤‡∏ç‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡∏ó‡∏≤‡∏á"]
            };

            // Find top 3 skills
            const topSkills = Object.entries(skillScores)
                .sort(([,a], [,b]) => b - a)
                .slice(0, 3);

            let recommendations = [];
            topSkills.forEach(([skill, score]) => {
                if (score >= 60 && careers[skill]) {
                    recommendations = recommendations.concat(careers[skill]);
                }
            });

            // Remove duplicates and limit to 6 recommendations
            return [...new Set(recommendations)].slice(0, 6);
        }

        function createSkillChart(skillScores) {
            const ctx = document.getElementById('skillChart').getContext('2d');
            
            new Chart(ctx, {
                type: 'radar',
                data: {
                    labels: Object.values(skillCategories),
                    datasets: [{
                        label: '‡∏Ñ‡∏∞‡πÅ‡∏ô‡∏ô‡∏ó‡∏±‡∏Å‡∏©‡∏∞‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì',
                        data: Object.values(skillScores),
                        backgroundColor: 'rgba(59, 130, 246, 0.2)',
                        borderColor: 'rgb(59, 130, 246)',
                        borderWidth: 2,
                        pointBackgroundColor: 'rgb(59, 130, 246)',
                        pointBorderColor: '#fff',
                        pointHoverBackgroundColor: '#fff',
                        pointHoverBorderColor: 'rgb(59, 130, 246)'
                    }]
                },
                options: {
                    responsive: true,
                    scales: {
                        r: {
                            beginAtZero: true,
                            max: 100,
                            ticks: {
                                stepSize: 20
                            }
                        }
                    },
                    plugins: {
                        legend: {
                            display: false
                        }
                    }
                }
            });
        }

        function createDemandChart() {
            const ctx = document.getElementById('demandChart').getContext('2d');
            
            new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: Object.values(skillCategories),
                    datasets: [{
                        label: '‡∏Ñ‡∏ß‡∏≤‡∏°‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÉ‡∏ô‡∏ï‡∏•‡∏≤‡∏î‡πÅ‡∏£‡∏á‡∏á‡∏≤‡∏ô (%)',
                        data: [75, 85, 90, 80, 95, 88, 92, 70],
                        backgroundColor: [
                            'rgba(59, 130, 246, 0.8)',
                            'rgba(16, 185, 129, 0.8)',
                            'rgba(245, 158, 11, 0.8)',
                            'rgba(239, 68, 68, 0.8)',
                            'rgba(139, 92, 246, 0.8)',
                            'rgba(236, 72, 153, 0.8)',
                            'rgba(34, 197, 94, 0.8)',
                            'rgba(251, 146, 60, 0.8)'
                        ],
                        borderColor: [
                            'rgb(59, 130, 246)',
                            'rgb(16, 185, 129)',
                            'rgb(245, 158, 11)',
                            'rgb(239, 68, 68)',
                            'rgb(139, 92, 246)',
                            'rgb(236, 72, 153)',
                            'rgb(34, 197, 94)',
                            'rgb(251, 146, 60)'
                        ],
                        borderWidth: 1
                    }]
                },
                options: {
                    responsive: true,
                    scales: {
                        y: {
                            beginAtZero: true,
                            max: 100,
                            ticks: {
                                callback: function(value) {
                                    return value + '%';
                                }
                            }
                        }
                    },
                    plugins: {
                        legend: {
                            display: false
                        },
                        tooltip: {
                            callbacks: {
                                label: function(context) {
                                    return context.parsed.y + '%';
                                }
                            }
                        }
                    }
                }
            });
        }

        function showSkillDetail(skillType) {
            const skillDetails = {
                financial: {
                    title: "‡∏Å‡∏≤‡∏£‡∏ö‡∏£‡∏¥‡∏´‡∏≤‡∏£‡πÄ‡∏á‡∏¥‡∏ô",
                    description: "‡∏ó‡∏±‡∏Å‡∏©‡∏∞‡πÉ‡∏ô‡∏Å‡∏≤‡∏£‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏Å‡∏≤‡∏£‡πÄ‡∏á‡∏¥‡∏ô‡∏™‡πà‡∏ß‡∏ô‡∏ï‡∏±‡∏ß‡πÅ‡∏•‡∏∞‡∏ò‡∏∏‡∏£‡∏Å‡∏¥‡∏à ‡∏£‡∏ß‡∏°‡∏ñ‡∏∂‡∏á‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏á‡πÅ‡∏ú‡∏ô‡∏Å‡∏≤‡∏£‡πÄ‡∏á‡∏¥‡∏ô ‡∏Å‡∏≤‡∏£‡∏•‡∏á‡∏ó‡∏∏‡∏ô ‡πÅ‡∏•‡∏∞‡∏Å‡∏≤‡∏£‡∏õ‡∏£‡∏∞‡∏´‡∏¢‡∏±‡∏î",
                    tips: [
                        "‡πÄ‡∏£‡∏µ‡∏¢‡∏ô‡∏£‡∏π‡πâ‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏ö‡∏õ‡∏£‡∏∞‡∏°‡∏≤‡∏ì‡∏£‡∏≤‡∏¢‡πÄ‡∏î‡∏∑‡∏≠‡∏ô",
                        "‡∏®‡∏∂‡∏Å‡∏©‡∏≤‡πÄ‡∏£‡∏∑‡πà‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏•‡∏á‡∏ó‡∏∏‡∏ô‡πÄ‡∏ö‡∏∑‡πâ‡∏≠‡∏á‡∏ï‡πâ‡∏ô",
